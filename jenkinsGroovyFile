pipeline
{
	agent any
	tools
	{
		maven 'MAVEN_HOME'
	}
	stages
	{
		stage('Welcome stage')
		{
			steps
				{
					echo 'Welcome to pipeline'
				}
		}
		stage('Clean stage')
		{
			steps
				{
					bat 'mvn clean'
				}
		}
		stage('Build stage')
		{
			steps
				{
					bat 'mvn install'
				}
		}
		stage('Test stage')
		{
			steps
				{
					bat 'mvn test'
				}
		}
		stage('Success stage')
		{
			steps
				{
					echo 'Build Successfully completed'
				}
		}
	}
}